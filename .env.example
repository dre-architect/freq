# Example environment variables for FREQ AI
# Copy this file to .env and update with your values

# Backend Configuration
LOG_LEVEL=INFO
PYTHONUNBUFFERED=1

# Point Cloud Processing
MAX_DEPTH=10.0
MIN_DEPTH=0.1
VOXEL_SIZE=0.01

# Validator Agent
MAX_DRAFT=15.0
MIN_POINTS=1000
CONFIDENCE_THRESHOLD=0.85

# Vector Computer
STEP_SIZE=0.1
SAFETY_MARGIN=0.2
SMOOTHING_FACTOR=0.5

# G-Code Generator
SAFE_HEIGHT=2.0
FEED_RATE=100.0
CRANE_TYPE=generic

# API Configuration
API_HOST=0.0.0.0
API_PORT=5000
API_BASE_URL=/api/v1

# Frontend Configuration (used during build)
VITE_API_URL=http://localhost:5000/api/v1
VITE_WS_URL=ws://localhost:5000/ws

# CesiumJS (optional)
# Get your token from https://cesium.com/ion/
CESIUM_ION_TOKEN=

# Database (if needed in future)
# DATABASE_URL=postgresql://user:password@localhost/freq_ai

# Redis (if needed for caching)
# REDIS_URL=redis://localhost:6379/0

# Monitoring (optional)
# SENTRY_DSN=
# NEW_RELIC_LICENSE_KEY=

# Development
DEBUG=False
TESTING=False
